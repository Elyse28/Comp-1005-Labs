def text_num():
    text = input("Enter a string: ")
    num = int(input("Enter a number between 1 and 10: "))
    while num <= 1 or num >= 10:
        print("Error: Number must be between 1 and 10.")
        num = int(input("Enter a number between 1 and 10: "))
    print(text * num)
    
text_num()